<div class="container">  <div class="row">    <div class="col-xs-12 col-sm-8 offset-sm-2">      <h1>Games</h1>      <% @games.each do |game| %>        <% @img = cloudinary_url(game.photo.key) %>        <% @card_title = link_to(game.title, game_path(game)) %>        <% @card_description = game.description %>        <% @card_owner = game.user.username %>        <%= render 'shared/card' %>      <% end %>    </div>  </div></div>